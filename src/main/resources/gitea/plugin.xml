<?xml version="1.0" encoding="utf-8" ?>
<go-plugin id="gitea.pr" version="1">
    <about>
        <name>Gitea Pull Requests Builder</name>
        <version>${version}</version>
        <target-go-version>19.2.0</target-go-version>
        <description>Plugin that polls a Gitea repository for pull requests and triggers a build for each of them</description>
        <vendor>
            <name>Helge Waastad</name>
            <url>https://github.com/hwaastad</url>
        </vendor>
    </about>
</go-plugin>